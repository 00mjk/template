{
  "branches": ["production"],
  "plugins": [
    [
      "semantic-release-gitmoji",
      {
        "releaseRules": {
          "patch": {
            "include": [":bento:", ":recycle:"]
          }
        }
      }
    ],
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    [
      "@semantic-release/git",
      { "message": ":bookmark: v${nextRelease.version} [skip ci]" }
    ]
  ]
}
